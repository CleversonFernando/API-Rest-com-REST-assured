{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "$id": "http://example.com/example.json",
  "type": "array",
  "default": [],
  "title": "Root Schema",
  "items": {
    "type": "object",
    "title": "A Schema",
    "required": [
      "id",
      "name",
      "age"
    ],
    "properties": {
      "id": {
        "type": "integer",
        "title": "The id Schema",
        "examples": [
          1,
          2,
          3
        ]
      },
      "name": {
        "type": "string",
        "title": "The name Schema",
        "examples": [
          "João da Silva",
          "Maria Joaquina",
          "Ana Júlia"
        ]
      },
      "age": {
        "type": "integer",
        "title": "The age Schema",
        "examples": [
          30,
          25,
          20
        ]
      },
      "salary": {
        "type": [
          "number",
          "integer"
        ],
        "title": "The salary Schema",
        "examples": [
          1234.5678,
          2500
        ]
      },
      "endereco": {
        "type": "object",
        "default": {},
        "title": "The endereco Schema",
        "required": [
          "rua",
          "numero"
        ],
        "properties": {
          "rua": {
            "type": "string",
            "default": "",
            "title": "The rua Schema",
            "examples": [
              "Rua dos bobos"
            ]
          },
          "numero": {
            "type": "integer",
            "default": 0,
            "title": "The numero Schema",
            "examples": [
              0
            ]
          }
        },
        "examples": [{
          "rua": "Rua dos bobos",
          "numero": 0
        }]
      },
      "filhos": {
        "type": "array",
        "default": [],
        "title": "The filhos Schema",
        "items": {
          "type": "object",
          "title": "A Schema",
          "required": [
            "name"
          ],
          "properties": {
            "name": {
              "type": "string",
              "title": "The name Schema",
              "examples": [
                "Zezinho",
                "Luizinho"
              ]
            }
          },
          "examples": [{
            "name": "Zezinho"
          },
            {
              "name": "Luizinho"
            }]
        },
        "examples": [
          [{
            "name": "Zezinho"
          },
            {
              "name": "Luizinho"
            }]
        ]
      }
    },
    "examples": [{
      "id": 1,
      "name": "João da Silva",
      "age": 30,
      "salary": 1234.5678
    },
      {
        "id": 2,
        "name": "Maria Joaquina",
        "endereco": {
          "rua": "Rua dos bobos",
          "numero": 0
        },
        "age": 25,
        "salary": 2500
      },
      {
        "id": 3,
        "name": "Ana Júlia",
        "age": 20,
        "filhos": [{
          "name": "Zezinho"
        },
          {
            "name": "Luizinho"
          }]
      }]
  },
  "examples": [
    [{
      "id": 1,
      "name": "João da Silva",
      "age": 30,
      "salary": 1234.5678
    },
      {
        "id": 2,
        "name": "Maria Joaquina",
        "endereco": {
          "rua": "Rua dos bobos",
          "numero": 0
        },
        "age": 25,
        "salary": 2500
      },
      {
        "id": 3,
        "name": "Ana Júlia",
        "age": 20,
        "filhos": [{
          "name": "Zezinho"
        },
          {
            "name": "Luizinho"
          }]
      }]
  ]
}